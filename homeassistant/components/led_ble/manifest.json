{
  "domain": "led_ble",
  "name": "LED BLE",
  "config_flow": true,
  "documentation": "https://www.home-assistant.io/integrations/led_ble/",
  "requirements": ["led-ble==1.0.0"],
  "dependencies": ["bluetooth"],
  "codeowners": ["@bdraco"],
  "bluetooth": [
    { "local_name": "LEDnet*" },
    { "local_name": "BLE-LED*" },
    { "local_name": "LEDBLE*" },
    { "local_name": "Triones*" },
    { "local_name": "LEDBlue*" },
    { "local_name": "Dream~*" },
    { "local_name": "QHM-*" },
    { "local_name": "AP-*" }
  ],
  "iot_class": "local_polling"
}
